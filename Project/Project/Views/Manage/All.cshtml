@model IEnumerable<ApplicationUser>

<form method="post" asp-action="Manage">
    <input type="text" placeholder="Email" name="id" />
    <input type="submit" />
</form>

<div class="col-md-12">
    <table class="table table-hover">
        <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Validate</th>
        </tr>
        </thead>
        <tbody>
            @foreach (var el in Model)
            {
                <tr>
                    <td>@el.FirstName @el.LastName</td>
                    <td>@el.Email</td>
                    <td>✓ ❌</td>
                </tr>
            }
        </tbody>
    </table>
</div>